# .env.example
# ВАЖНО: Никогда не коммитьте реальные ключи в Git!

# Способ 1: JSON ключ как переменная окружения (рекомендуется для продакшена)
GOOGLE_SERVICE_ACCOUNT_JSON={"type": "service_account", "project_id": "your-project", ...}

# Способ 2: Путь к файлу ключа (для разработки)
GOOGLE_KEY_FILE_PATH=./config/service-account-key.json

# ID представления Google Analytics
ANALYTICS_VIEW_ID=123456789

# Настройки логирования
LOG_LEVEL=INFO
